{
    "name": "angel-source-labs/laravel-expressions",
    "description": "Provides expressions with bindings and expressions that can be used as column values in eloquent",
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "test:unit": "phpunit -c phpunit.xml.dist --testsuite unit",
        "test:integration": "phpunit -c phpunit.xml.dist --testsuite integration"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Brion Finlay"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AngelSourceLabs\\LaravelExpressions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
